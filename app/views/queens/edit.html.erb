<h1>Editing queen</h1>

<% form_for(@queen) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :kingdom %><br />
    <%= f.text_field :kingdom %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @queen %> |
<%= link_to 'Back', queens_path %>